/dts-v1/;
#include "cv181x_base_riscv.dtsi"
#include "cv181x_asic_bga.dtsi"
#include "cv181x_asic_emmc.dtsi"
#include "cv181x_default_memmap.dtsi"
#include "cv181x_base.dtsi"

#include "cv1813h_lubancat_keys.dtsi"

/ {
    	leds: leds {
			status = "okay";
			compatible = "gpio-leds";

			sys_status_led: sys-status-led {
				label = "sys_status_led";
				linux,default-trigger = "heartbeat";
				default-state = "on";
				gpios = <&portb 11 GPIO_ACTIVE_LOW>;
				};
			};

		usb_host_en: usb_host_en {
			compatible = "regulator-gpio";
			regulator-name = "usb_host_en";
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <1800000>;
			gpios = <&porte 13 GPIO_ACTIVE_HIGH>;
			regulator-always-on;
			regulator-boot-on;
			enable-active-high;
		};
};

&wifisd {
		status = "okay";
};

&wifi_pin {
	poweron-gpio = <&porta 4 GPIO_ACTIVE_HIGH>;
	wakeup-gpio = <&porta 0 GPIO_ACTIVE_HIGH>;
};
